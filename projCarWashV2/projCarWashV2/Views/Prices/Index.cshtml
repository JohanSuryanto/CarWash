@model IEnumerable<projCarWashV2.Price>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name_Price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Small)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Medium)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Large)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name_Price)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Small)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Medium)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Large)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_Type }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID_Type }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID_Type })
        </td>
    </tr>
}

</table>
